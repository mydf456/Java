package operator;

public class OperatorTest4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 조건 연산자, 삼항 연산자 
		//      조건식 ? true : false
		int i = 10<20 ? 10 : 20;
		System.out.println(i);
		String str = 10<20 ? "참" : "거짓";
		System.out.println(str);
	}

}






