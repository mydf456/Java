package classtest;

public class FunctionTest {
	//1부터 파라미터로 받아온 값까지 더해주는 메서드를 작성해보세요.
	int fac(int value) {
		int result = 0;
		for(int i=0; i<=value; i++) {
			//result += i;
			result = result + i;
		}
		return result;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		FunctionTest ft = new FunctionTest();
	}

}







