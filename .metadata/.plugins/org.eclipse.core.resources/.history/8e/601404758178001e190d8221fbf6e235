package operator;

public class OperatorTest3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// +=, -=, *=, /=, %= : 복합 대입 연산자
		int i = 10;
		int j = 20;
		//i = i+j;
		System.out.println(i+=j); // i에 j를 더하여 저장
		System.out.println(i-=j);
		System.out.println(i*=j);
		System.out.println(i/=j);
		System.out.println(i%=j);
		
	}

}






