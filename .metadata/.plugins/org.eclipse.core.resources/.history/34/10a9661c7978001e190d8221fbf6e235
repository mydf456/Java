package operator;

public class OperatorTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// = : 대입 연산자, 변수에 값을 대입하는 역할
		int i = 24;
		// +, - 부호 연산자
		i = +1;
		i = -1;
		//+-*/% : 산술연산자
		i = 1+2; // 3
		System.out.println(i);
		i = 1-2; // -1
		System.out.println(i);
		i = 10*2; // 20
		System.out.println(i);
		i = 10 / 2; // 5
		System.out.println(i);
		i = 10%3; // 1
		System.out.println(i);
	}

}







