package switchtest;

import java.util.Scanner;

public class SwitchTest2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 1~12월 까지의 일수를 출력하는 프로그램 작성하기
		// 1,3,5,7,8,10,12 = 31일
		// 4,6,9,11 = 30일
		// 2 = 28일
		Scanner sc = new Scanner(System.in);
		int month = sc.nextInt();
	}

}













